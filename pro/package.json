{
  "name": "pass-culture-pro",
  "version": "0.0.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html build/stats.html",
    "build": "yarn copy-assets && react-scripts build",
    "build:development": "env-cmd -f .env.development yarn build",
    "build:staging": "env-cmd -f .env.staging yarn build",
    "build:testing": "env-cmd -f .env.testing yarn build",
    "build:integration": "env-cmd -f .env.integration yarn build",
    "build:production": "env-cmd -f .env.production yarn build",
    "generate:api:client": "./scripts/generate_api_client.sh",
    "generate:api:client:local": "./scripts/generate_api_client_local.sh",
    "lint:js": "eslint '{src,testcafe}/**/*.{js,jsx,ts,tsx}'",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:scss:fix": "yarn lint:scss --fix",
    "lint:scss:hook": "stylelint",
    "lint:scss:hook:fix": "yarn lint:scss:hook --fix",
    "lint:dead-code": "ts-unused-exports tsconfig.json --excludePathsFromReport='stories;apiClient'",
    "prettier:js:changed:fix": "./scripts/lint_diff.sh",
    "serve": "serve -s build -l 3001",
    "start": "yarn copy-assets && PORT=3001 react-scripts start",
    "test:cafe": "yarn testcafe testcafe",
    "test:e2e": "cypress open --e2e --browser chrome --config baseUrl=http://localhost:3001",
    "test:stylelint": "jest stylelint-pass-culture/tests",
    "test:unit": "TZ=UTC jest --coverage",
    "test:unit:ci": "TZ=UTC jest --max-workers=2",
    "test:unit:one": "TZ=UTC jest --bail",
    "test:unit:watch": "TZ=UTC jest --watch",
    "test:unit:no-coverage": "TZ=UTC jest",
    "testcafe": "NODE_ENV=test testcafe --cache --assertion-timeout 1000 --selector-timeout 5000 -s path=testcafe_screenshots,takeOnFails=true 'chrome:headless'",
    "testcafe-firefox": "NODE_ENV=test testcafe --assertion-timeout 1000 --selector-timeout 5000 -s path=testcafe_screenshots,takeOnFails=true 'firefox:headless'",
    "testcafe-firefox:debug": "NODE_ENV=test testcafe --assertion-timeout 1000 --selector-timeout 5000 -s path=testcafe_screenshots,takeOnFails=true 'firefox' --live --sf",
    "testcafe-chromium": "NODE_ENV=test testcafe --assertion-timeout 1000 --selector-timeout 5000 -s path=testcafe_screenshots,takeOnFails=true 'chromium:headless'",
    "testcafe-chromium:debug": "NODE_ENV=test testcafe --assertion-timeout 1000 --selector-timeout 5000 -s path=testcafe_screenshots,takeOnFails=true 'chromium' --live --sf",
    "storybook": "yarn copy-assets && NODE_PATH=src start-storybook -p 6006",
    "build-storybook": "yarn copy-assets && NODE_PATH=src build-storybook -o docs-build",
    "copy-assets": "cp ./src/icons/*.svg ./public/icons/"
  },
  "dependencies": {
    "@firebase/analytics": "^0.10.0",
    "@firebase/app": "^0.9.12",
    "@firebase/remote-config": "0.4.4",
    "@reach/dialog": "^0.18.0",
    "@reach/menu-button": "^0.18.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/browser": "^7.53.1",
    "@sentry/react": "^7.54.0",
    "@sentry/tracing": "^7.54.0",
    "@zootools/email-spell-checker": "^1.12.0",
