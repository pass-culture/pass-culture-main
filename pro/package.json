{
  "name": "@pass-culture/pass-culture-pro",
  "version": "0.0.0",
  "private": true,
  "homepage": "/",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:development": "VITE_APP_VERSION=$(node -pe 'require(\"./package.json\").version') vite build --mode development",
    "build:staging": "VITE_APP_VERSION=$(node -pe 'require(\"./package.json\").version') vite build --mode staging",
    "build:testing": "VITE_APP_VERSION=$(node -pe 'require(\"./package.json\").version') vite build --mode testing",
    "build:integration": "VITE_APP_VERSION=$(node -pe 'require(\"./package.json\").version') vite build --mode integration",
    "build:production": "VITE_APP_VERSION=$(node -pe 'require(\"./package.json\").version') vite build --mode production",
    "generate:api:client:local": "./scripts/generate_api_client_local.sh",
    "generate:api:client:local:no:docker": "PCAPI_HOST=localhost:5001 ./scripts/generate_api_client_local.sh",
    "lint:js": "biome check --no-errors-on-unmatched",
    "lint:js:fix": "yarn lint:js --write",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:scss:fix": "yarn lint:scss --fix",
    "lint:scss:hook": "stylelint",
    "lint:scss:hook:fix": "yarn lint:scss:hook --fix",
    "lint:dead-code": "ts-unused-exports tsconfig.json --excludePathsFromReport='stories;apiClient;vite.config.ts'",
    "prettier:js:changed:fix": "./scripts/lint_diff.sh",
    "serve": "vite preview",
    "start": "vite",
    "test:bdd": "cypress run --e2e --browser firefox --env tags=@P0 --config-file cypress/cypress.config.ts",
    "test:e2e": "cypress open --e2e --browser firefox --config-file cypress/cypress.config.ts",
    "test:e2e:playwright": "playwright test -c ./config/playwright.config.ts --headed --project=chromium",
    "test:e2e:playwright:ci": "playwright test -c ./config/playwright.config.ts --project=chromium",
    "test:e2e:playwright:debug": "playwright test -c ./config/playwright.config.ts --debug --project=chromium",
    "test:e2e:playwright:mobile": "playwright test -c ./config/playwright.config.ts --headed --project=mobile-chrome",
    "test:e2e:playwright:mobile:ci": "playwright test -c ./config/playwright.config.ts --project=mobile-chrome",
    "test:e2e:playwright:mobile:debug": "playwright test -c ./config/playwright.config.ts --debug --project=mobile-chrome",
    "test:unit": "vitest",
    "test:unit:hook": "vitest related --run",
    "test:unit:hookcoverage": "vitest related --run --coverage.enabled=true --coverage.100=true",
    "test:unit:ci": "vitest run --coverage",
    "storybook": "NODE_PATH=src storybook dev -p 6006",
    "build-storybook": "NODE_PATH=src storybook build -o docs-build && node scripts/fixStorybookBuild.js"
  },
  "dependencies": {
    "@algolia/autocomplete-plugin-query-suggestions": "1.19.4",
    "@algolia/autocomplete-plugin-recent-searches": "1.19.4",
    "@firebase/analytics": "0.10.19",
    "@firebase/app": "0.14.6",
    "@firebase/remote-config": "0.7.0",
    "@hookform/resolvers": "5.2.2",
    "@pass-culture/design-system": "0.5.2",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@reduxjs/toolkit": "2.11.1",
    "@sentry/browser": "10.31.0",
    "@sentry/react": "10.31.0",
    "@zootools/email-spell-checker": "1.12.0",
    "chart.js": "4.5.1",
    "chartjs-adapter-date-fns": "3.0.0",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "dompurify": "3.3.1",
    "downshift": "9.0.13",
    "libphonenumber-js": "1.12.31",
    "react": "18.3.1",
    "react-avatar-editor": "13.0.2",
    "react-chartjs-2": "5.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "14.3.8",
    "react-hook-form": "7.68.0",
    "react-instantsearch": "7.21.0",
    "react-redux": "9.2.0",
    "react-router": "7.10.1",
    "swr": "2.3.7",
    "use-debounce": "10.0.6",
    "uuidv4": "6.2.13",
    "yup": "1.7.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
