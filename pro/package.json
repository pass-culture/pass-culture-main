{
  "name": "pass-culture-pro",
  "version": "latest-master",
  "private": true,
  "homepage": "/",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html build/stats.html",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging yarn build",
    "build:testing": "env-cmd -f .env.testing yarn build",
    "build:integration": "env-cmd -f .env.integration yarn build",
    "build:production": "env-cmd -f .env.production yarn build",
    "generate:api:client": "./scripts/generate_api_client.sh",
    "generate:api:client:local": "./scripts/generate_api_client_local.sh",
    "licenses": "node_modules/.bin/license-reporter save --json licenses.json",
    "lint:js": "eslint '{src,testcafe}/**/*.{js,jsx,ts,tsx}' --max-warnings=0",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:scss:fix": "yarn lint:scss --fix",
    "prettier:js:changed:fix": "./scripts/lint_diff.sh",
    "qa:diff": "scripts/compare-screenshots.sh",
    "qa:test": "yarn testcafe quality_assurance",
    "start": "PORT=3001 react-scripts start",
    "test:cafe": "yarn testcafe testcafe",
    "test:eslint": "jest eslint-plugin-pass-culture/tests",
    "test:stylelint": "jest stylelint-pass-culture/tests",
    "test:unit": "NODE_ICU_DATA=node_modules/full-icu TZ=UTC jest",
    "test:unit:ci": "NODE_ICU_DATA=node_modules/full-icu TZ=UTC jest --max-workers=2",
    "test:unit:one": "NODE_ICU_DATA=node_modules/full-icu TZ=UTC jest --bail",
    "testcafe": "testcafe --assertion-timeout 5000 --selector-timeout 15000 -S -s testcafe_screenshots 'chrome:headless'",
    "testcafe-firefox": "testcafe --assertion-timeout 5000 --selector-timeout 15000 -S -s testcafe_screenshots 'firefox:headless'",
    "testcafe-chromium": "testcafe --assertion-timeout 5000 --selector-timeout 15000 -S -s testcafe_screenshots 'chromium:headless'",
    "storybook": "NODE_PATH=src start-storybook -s ./public -p 6006",
    "build-storybook": "NODE_PATH=src build-storybook -o docs-build -s ./src/stories/assets",
    "debt:check:changed": "debt-collector check -c ./.debt-collector/config.js -s master",
    "debt:check:all": "debt-collector check -c ./.debt-collector/config.js",
    "debt:compare": "debt-collector compare -c ./.debt-collector/config.js --revision master --output-html",
    "debt:walk": "debt-collector walk -c ./.debt-collector/config.js -n 10",
    "new:component": "hygen new component"
  },
  "dependencies": {
    "@firebase/analytics": "^0.7.7",
