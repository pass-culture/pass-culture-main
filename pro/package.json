    "react-avatar-editor": "^13.0.2",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.0.0",
    "react-instantsearch": "^7.7.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "reselect": "^5.1.0",
