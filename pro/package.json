    "@sentry/react": "^7.103.0",
    "@sentry/tracing": "^7.103.0",
    "@zootools/email-spell-checker": "^1.12.0",
    "chart.js": "^4.4.2",
    "chartjs-adapter-date-fns": "^3.0.0",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "downshift": "^8.4.0",
    "formik": "^2.4.5",
    "libphonenumber-js": "1.10.58",
    "lodash.invert": "^4.3.0",
    "lodash.isequal": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "orejime": "^2.3.0",
    "papaparse": "^5.4.1",
    "react": "^18.0.0",
    "react-autocomplete": "^1.8.1",
