    "libphonenumber-js": "^1.10.34",
    "lodash.debounce": "^4.0.8",
    "lodash.invert": "^4.3.0",
    "lodash.isequal": "^4.5.0",
    "orejime": "^2.2.1",
    "papaparse": "^5.4.1",
    "prop-types": "^15.8.1",
    "re-reselect": "^4.0.1",
    "react": "^18.0.0",
    "react-autocomplete": "^1.8.1",
    "react-autosize-textarea": "^7.1.0",
    "react-avatar-editor": "^13.0.0",
    "react-csv": "^2.2.2",
    "react-datepicker": "^4.13.0",
    "react-dom": "^18.0.0",
