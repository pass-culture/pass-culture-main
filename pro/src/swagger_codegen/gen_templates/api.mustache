import { getAuthenticationHeaders, handleGeneratedApiResponse, safeFetch } from 'api/apiHelpers'
import { E2E_LOCALHOST_ADDRESS, E2E_LOCALHOST_ADDRESS_ANDROID } from 'libs/e2e/constants'
