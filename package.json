{
  "name": "pass-culture-pro",
  "version": "110.0.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "build": "node scripts/build.js && ./scripts/build_redirects.sh",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "licenses": "node_modules/.bin/license-reporter save --json licenses.json",
    "lint:js": "eslint '{src,testcafe}/**/*.{js,jsx}'",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:scss": "yarn stylelint '**/*.scss'",
    "lint:scss:fix": "yarn lint:scss --fix",
    "start": "node scripts/start.js",
    "start-with-cra": "react-scripts start",
    "symlink-shared": "if [ -d \"../shared\" ]; then cd node_modules && rm -rf pass-culture-shared && ln -sf ../../shared pass-culture-shared && cd ../../shared && yarn run compile; fi",
    "test:cafe": "testcafe --assertion-timeout 5000 --selector-timeout 15000 -S -s testcafe_screenshots 'chrome:headless' testcafe",
    "test:unit": "NODE_ICU_DATA=node_modules/full-icu TZ=UTC jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.svg": [
      "svgo --disable=removeViewBox"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --max-warnings=0 --fix"
    ]
  },
  "dependencies": {
    "@sentry/browser": "^5.27.3",
    "@sentry/tracing": "^5.27.3",
    "bulma": "0.7.4",
    "classnames": "^2.2.6",
    "final-form": "^4.20.1",
    "final-form-calculate": "^1.3.2",
    "full-icu": "^1.3.1",
    "ibantools": "^2.0.0",
    "leaflet": "^1.7.1",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.invert": "^4.3.0",
    "lodash.merge": "4.6.2",
    "lodash.omit": "4.5.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.21",
    "pass-culture-shared": "0.1.7",
    "promise": "^8.1.0",
    "prop-types": "^15.6.0",
    "query-string": "5.1.1",
    "raf": "^3.4.1",
    "re-reselect": "3.4.0",
    "react": "^16.10.2",
    "react-autocomplete": "^1.8.1",
    "react-autosize-textarea": "^7.1.0",
    "react-avatar-editor": "augnustin/react-avatar-editor#pass-culture",
    "react-csv": "^2.0.3",
    "react-datepicker": "^1.8.0",
    "react-dom": "^16.13.1",
    "react-dotdotdot": "^1.2.3",
    "react-final-form": "^5.1.2",
    "react-final-form-utils": "1.4.0",
    "react-hot-loader": "^3.1.3",
    "react-leaflet": "^2.7.0",
    "react-loading-infinite-scroller": "1.5.1",
    "react-portal": "^4.2.1",
    "react-redux": "^5.0.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.0.0",
    "react-scripts": "^4.0.0",
    "react-table": "^7.5.2",
    "react-time-input": "^0.0.19",
    "react-tooltip": "^3.8.4",
    "react-tracking": "^7.1.0",
    "redux": "^4.0.5",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.1.3",
    "redux-saga-data": "3.3.0",
    "redux-thunk": "^2.3.0",
    "reselect": "4.0.0",
    "typeface-barlow": "^0.0.54",
    "whatwg-fetch": "^3.4.1",
    "with-query-router": "3.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.14",
    "autoprefixer": "9.6.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "6.26.0",
    "chalk": "2.4.2",
    "commander": "^2.14.1",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-loader": "^4.0.2",
    "history": "^4.7.2",
    "husky": "^3.0.3",
    "input-range-scss": "1.5.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-fetch-mock": "^2.1.2",
    "license-reporter": "^1.3.0",
    "lint-staged": "^10.3.0",
    "node-env-file": "^0.1.8",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.13.1",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "svgo": "^1.3.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "testcafe": "^1.9.4",
    "uglifyjs-webpack-plugin": "^2.2.0"
  },
  "engines": {
    "node": ">=12 <13"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
